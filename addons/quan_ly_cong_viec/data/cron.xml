<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="cron_send_deadline_warnings" model="ir.cron">
        <field name="name">Cảnh Báo Công Việc Sắp Hết Hạn</field>
        <field name="model_id" ref="quan_ly_cong_viec.model_cong_viec"/>
        <field name="state">code</field>
        <field name="code">model.send_deadline_warnings()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
    </record>
</odoo>

